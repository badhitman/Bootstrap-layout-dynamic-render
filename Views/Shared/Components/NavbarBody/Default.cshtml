@using BootstrapViewComponentsRazorLibrary.Service.bootstrap.navbar
@using BootstrapViewComponentsRazorLibrary.Service
@model NavbarBodyManager
@foreach (AbstractDomManager adm in Model.GetChilds())
{
    @if (adm is NavbarNavManager)
    {
        adm.AddCSS("navbar-nav");
    }
    @await Component.InvokeAsync(adm.ViewComponentName, new { SetObjectManager = adm })
}