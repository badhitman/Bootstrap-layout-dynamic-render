@using BootstrapViewComponentsRazorLibrary.Service.html.input
@model InputSelectManager
<select @Html.Raw(Model.GetStringAttributes())>
    @foreach (InputOptionSelectManager opt in Model.Options)
    {
        @await Component.InvokeAsync(opt.ViewComponentName, new { SetObjectManager = opt })
    }
</select>