@using BootstrapAspDynamicRender.service
@using BootstrapAspDynamicRender.components
@model hsFieldset
<fieldset @Html.Raw(Model.GetStringAttributes())>
    @if (!string.IsNullOrWhiteSpace(Model.Legend))
    {
        <legend>@Html.Raw(Model.Legend)</legend>
    }
    @await Component.InvokeAsync(typeof(hrUniversalList).Name, new { SetObjectManager = Model })
</fieldset>