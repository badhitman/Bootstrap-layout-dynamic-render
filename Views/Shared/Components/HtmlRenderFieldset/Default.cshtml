@using BootstrapViewComponentsRazorLibrary.Service.html
@using BootstrapViewComponentsRazorLibrary.Components.html
@model FieldsetManager
<fieldset @Html.Raw(Model.GetStringAttributes())>
    @if (!string.IsNullOrWhiteSpace(Model.Legend))
    {
        <legend>@Html.Raw(Model.Legend)</legend>
    }
    @await Component.InvokeAsync(typeof(HtmlRenderUniversalList).Name, new { SetObjectManager = Model })
</fieldset>